{
  "name": "StreetSupportApp",
  "description": "Supporting homeless people together",
  "private": true,
  "scripts": {
    "run:a": "ionic cordova run android",
    "run:i": "ionic cordova run ios",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "test": "ts-mocha -p ./tsconfig.json ./src/**/*.spec.ts",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@auth0/cordova": "^0.3.0",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/geolocation": "^4.5.3",
    "@ionic-native/in-app-browser": "^4.5.3",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "^2.1.3",
    "auth0-js": "^9.10.0",
    "cordova-android": "^7.1.2",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-app-name": "^1.0.4",
    "cordova-plugin-customurlscheme": "4.3.0",
    "cordova-plugin-device": "^2.0.1",
    "cordova-plugin-geolocation": "^4.0.1",
    "cordova-plugin-inappbrowser": "^1.6.1",
    "cordova-plugin-ionic-keyboard": "^2.0.5",
    "cordova-plugin-ionic-webview": "^1.1.16",
    "cordova-plugin-safariviewcontroller": "1.5.4",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.1",
    "cordova-plugin-whitelist": "^1.3.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "marked": "^0.3.17",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^3.2.0",
    "@types/expect": "^1.20.3",
    "@types/mocha": "^5.2.5",
    "htmlencode": "0.0.4",
    "mocha": "^5.2.0",
    "ts-mocha": "^2.0.0",
    "typescript": "2.4.2"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard",
    "cordova-plugin-geolocation",
    {
      "id": "cordova-plugin-app-name",
      "locator": "cordova-plugin-app-name",
      "variables": {
        "APP_NAME": "Street Support"
      }
    },
    "cordova-plugin-inappbrowser"
  ],
  "cordovaPlatforms": [
    {
      "platform": "ios",
      "version": "4.3.1",
      "locator": "ios"
    },
    {
      "platform": "android",
      "version": "6.1.2",
      "locator": "android"
    }
  ],
  "cordova": {
    "plugins": {
      "cordova-plugin-geolocation": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-app-name": {
        "APP_NAME": "Street Support"
      },
      "cordova-plugin-statusbar": {},
      "cordova-plugin-safariviewcontroller": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "net.streetsupport.findhelpapp",
        "ANDROID_SCHEME": "net.streetsupport.findhelpapp",
        "ANDROID_HOST": "streetsupport.eu.auth0.com",
        "ANDROID_PATHPREFIX": "/cordova/net.streetsupport.findhelpapp/callback"
      }
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
